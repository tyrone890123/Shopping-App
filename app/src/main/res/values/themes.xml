<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <!--New theme used is the Material Design (No Action Bar) dependency in build.gradle Module Test app-->
    <style name="Theme.TestApp" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimary">@color/soothing</item>
    </style>

    <style name="Theme.TestApp.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.TestApp.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.TestApp.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>